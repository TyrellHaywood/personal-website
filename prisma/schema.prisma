
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model SoftwarePost {
  id    Int    @id @default(autoincrement())
  title String
  year  Int
  image String
  description String @db.Text
}

model HardwarePost {
  id    Int    @id @default(autoincrement())
  title String
  year  Int
  image String
  description String @db.Text
}

model CommunityPost {
  id    Int    @id @default(autoincrement())
  title String
  year  Int
  image String
  description String @db.Text
}

model PersonalPost {
  id    Int    @id @default(autoincrement())
  title String
  year  Int
  image String
  description String @db.Text
}
